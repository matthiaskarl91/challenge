{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "css:watch": "npm run build:css -- --watch",
    "dev": "cross-env NODE_ENV=development remix dev",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|ts|tsx)\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "remix setup node",
    "type-check": "tsc"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/react": "^5.10.1",
    "@remix-run/react": "^1.2.2",
    "@remix-run/vercel": "^1.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.2",
    "@remix-run/serve": "^1.2.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-kentcdodds": "^20.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "msw": "^0.36.8",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "tailwindcss": "^3.0.21",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
